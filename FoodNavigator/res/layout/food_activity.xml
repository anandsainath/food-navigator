<?xml version="1.0" encoding="utf-8"?>
<LinearLayout 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:ctc="http://schemas.android.com/apk/res/edu.gatech.hci.foodnavigator"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">

	<edu.gatech.hci.foodnavigator.utilities.ImageMap  
	    android:id="@+id/map"
    	android:layout_width="fill_parent" 
    	android:layout_height="fill_parent"
    	ctc:map="foodmap"
        ctc:maxSizeFactor="1"
        ctc:scaleFromOriginal="false"
        ctc:fitImageToScreen="false"/>

</LinearLayout>
